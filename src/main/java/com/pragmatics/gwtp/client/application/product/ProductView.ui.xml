<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui" xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<!-- <ui:with field="nameTokens" type="com.p"/> -->
	<ui:with field="nameTokens" type="com.pragmatics.gwtp.client.place.NameTokens" />


	<b:Panel>
		<b:PanelHeader>
			<b:Heading size="H3" text="Product Details" ui:field="heading" />
		</b:PanelHeader>
		<b:PanelBody>
			<b:ListGroup>
				<b:ListGroupItem text="..." ui:field="product">
				</b:ListGroupItem>
			</b:ListGroup>
			<b.html:Text ui:field="message" />
		</b:PanelBody>
		<b:PanelFooter>
			<b:Anchor ui:field="back" text="Back" />
		</b:PanelFooter>
	</b:Panel>





</ui:UiBinder>
